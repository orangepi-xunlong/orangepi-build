			item.11 DTS_HD
			item.12 MAT
			item.13 DST
			item.14 WMAPRO
		}
	}
	control.2 {
		iface MIXER
		name 'loopback debug'
		value false
		comment {
			access 'read write'
			type BOOLEAN
			count 1
		}
	}
}
